>\n    <!-- 主体部分 -->\n    <el-main>\n      <el-col :span=\"15\" class=\"login_banner\">\n        <el-image :src=\"image.login_banner\"></el-image>\n      </el-col>\n      <el-col :span=\"1\" style=\"margin-top: 6em; text-align: center;\">\n        <div class=\"banner-text\">\n          <span>商品美·种类多·欢迎选购</span>\n        </div>\n      </el-col>\n      <el-col :span=\"7\" class=\"login_banner\">\n        <div class=\"login_container\">\n          <div class=\"subs\">\n            <div class=\"login-text\">用&nbsp;户&nbsp;登&nbsp;陆</div>\n            <el-divider></el-divider>\n            <!-- 表单部分 -->\n            <div class=\"subs-subs\">\n              <el-form :model=\"form\" :rules=\"rules\" ref=\"from\">\n                <el-form-item prop=\"username\">\n                  <el-input\n                    v-model=\"form.username\"\n                    placeholder=\"请输用户名或手机号\"\n                    suffix-icon=\"el-icon-user\"\n                  ></el-input>\n                </el-form-item>\n                <el-form-item prop=\"password\">\n                  <el-input\n                    v-model=\"form.password\"\n                    type=\"password\"\n                    placeholder=\"请输入密码\"\n                    suffix-icon=\"el-icon-lock\"\n                  ></el-input>\n                </el-form-item>\n                <el-form-item class=\"keep-alive\">\n                  <el-checkbox v-model=\"keep\" true-label=\"true\">保持登陆</el-checkbox>\n                </el-form-item>\n                <el-form-item>\n                  <el-button type=\"danger\" style=\"width: 18em;\" @click=\"submit\">登&nbsp;陆</el-button>\n                </el-form-item>\n              </el-form>\n            </div>\n\n            <el-divider></el-divider>\n            <div>\n              <el-row>\n                <el-col :span=\"6\">\n                  <el-link href target=\"_blank\">\n                    <el-image :src=\"image.qq\" ></el-image>QQ登陆\n                  </el-link>\n                </el-col>\n                <el-col :span=\"8\">\n                  <el-link href target=\"_blank\">微信登陆</el-link>\n                </el-col>\n                <el-col :span=\"10\">\n                  <!-- click事件直接绑定router内置函数完成页面跳转 -->\n                  <el-button type=\"text\" @click=\"$router.push({ name: 'register' })\">\n                    立即注册\n                    <i class=\"el-icon-d-arrow-right el-icon--right\"></i>\n                  </el-button>\n                </el-col>\n              </el-row>\n            </div>\n          </div>\n        </div>\n      </el-col>\n    </el-main>\n    <!-- footer部分 -->\n    <el-footer style=\"height: 15%;\">\n      <p>\n        <el-link type=\"primary\">关于我们</el-link>\n        <el-divider direction=\"vertical\"></el-divider>\n        <el-link type=\"primary\">联系我们</el-link>\n        <el-divider direction=\"vertical\"></el-divider>\n        <el-link type=\"primary\">加入我们</el-link>\n        <el-divider direction=\"vertical\"></el-divider>\n        <el-link type=\"primary\">友情链接</el-link>\n      </p>\n      <p>CopyRight © 2016 北京美多商业股份有限公司 All Rights Reserved</p>\n      <p>电话：010-****888 京ICP备*******8号</p>\n    </el-footer>\n  </el-container>\n</template>\n\n<script>\nexport default {\n  name: \"Login\",\n  data() {\n    return {\n      // 本地图片资源\n      image: {\n        logo: require(\"../assets/images/logo02.png\"),\n        login_banner: require(\"../assets/images/login_banner.png\"),\n        qq: require(\"../assets/images/qq.jpg\"),\n        wx: require(\"../assets/images/wx.jpg\")\n      },\n\n      // 登陆表单\n      form: {\n        username: \"\",\n        password: \"\"\n      },\n      // 表单校验规则\n      rules: {\n        username: [\n          {\n            required: true,\n            message: \"  请填写用户名\",\n            trigger: \"blur\"\n          }\n        ],\n        password: [\n          {\n            required: true,\n            message: \"  请填写密码\",\n            trigger: \"blur\"\n          }\n        ]\n      },\n\n      // 保持登陆\n      keep: \"\"\n    };\n  },\n  methods: {\n    submit() {\n      this.$refs.from.validate(valid => {\n        if (valid) {\n          let that = this;\n\n          that.$axios\n            .post(\"login/\", that.form)\n            .then(res => {\n              if (res.status === 200) {\n                // 登陆成功后，保存token信息\n                sessionStorage.clear(); // sessionStorage关闭浏览器即清除\n                localStorage.clear(); // 永久保存\n\n                if (that.keep === \"true\") {\n                  localStorage.token = res.data.token;\n                  localStorage.username = res.data.username;\n                  localStorage.user_id = res.data.id;\n                } else {\n                  sessionStorage.token = res.data.token;\n                  sessionStorage.username = res.data.username;\n                  sessionStorage.user_id = res.data.id;\n                }\n\n                // 跳转页面\n                that.$router.push({ name: \"home\" });\n              } else {\n                that.$message.error(\"未知错误，请联系管理员\");\n              }\n            })\n            .catch(err => {\n              that.$message.error(\"账号或密码错误\");\n              console.log(err.message);\n            });\n        }\n      });\n    }\n  }\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n.el-container {\n  height: 100%;\n}\n.el-header {\n  background-color: white;\n}\n\n.el-footer {\n  background-color: white;\n  text-align: center;\n  font-size: smaller;\n}\n\n.el-main {\n  background-color: rgb(160, 9, 9);\n}\n\n.login_banner {\n  margin-top: 3em;\n  text-align: right;\n}\n.banner-text {\n  color: white;\n  width: 1em;\n}\n.login-text {\n  font-size: 1.5em;\n  color: red;\n  padding-top: 1em;\n}\n.el-input {\n  width: 18em;\n}\n.login_container {\n  background-color: white;\n  width: 22em;\n  height: 24em;\n}\n.subs {\n  text-align: center;\n  margin-left: 2em;\n  width: 18em;\n}\n.subs-subs {\n  text-align: center;\n  margin-left: 1em;\n  width: 16em;\n}\n.el-divider--horizontal {\n  margin-top: 0.5em;\n  margin-bottom: 1em;\n}\n.keep-alive {\n  margin-bottom: 0;\n  margin-left: 0.5em;\n  text-align: left;\n}\n.el-row {\n  text-align: right;\n}\n.el-row .el-button {\n  padding-top: 0;\n}\n.el-link .el-image {\n  width: 1em;\n  height: 1em;\n}\n</style>"]}]}