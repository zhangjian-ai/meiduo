sourcesContent":["<template>\n  <Container>\n    <Category></Category>\n    <!-- 面包屑 -->\n    <el-row>\n      <el-col :span=\"2\">\n        <pre style=\"margin:0; padding: 0;\">&nbsp;</pre>\n      </el-col>\n      <el-col :span=\"4\">\n        <el-breadcrumb style=\"font-size: 0.5em; color:red;\">\n          <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\n          <el-breadcrumb-item>商品详情</el-breadcrumb-item>\n        </el-breadcrumb>\n      </el-col>\n    </el-row>\n    <!-- 详情展示区 -->\n    <el-row>\n      <el-col :span=\"2\">\n        <pre style=\"margin:0; padding: 0;\">&nbsp;</pre>\n      </el-col>\n      <el-col :span=\"20\">\n        <div>\n          <el-col :span=\"8\">\n            <div>\n              <el-image :src=\"sku_data.default_image_url\"></el-image>\n            </div>\n            <div class=\"img-list\">\n              <el-scrollbar>\n                <ul>\n                  <li v-for=\"item in sku_data.images\" :key=\"item\">\n                    <el-image :src=\"item.image\"></el-image>\n                  </li>\n                </ul>\n              </el-scrollbar>\n            </div>\n          </el-col>\n        </div>\n      </el-col>\n    </el-row>\n  </Container>\n</template>\n<script>\nimport Container from \"@/components/Container.vue\";\nimport Category from \"@/components/Category.vue\";\n\nexport default {\n  name: \"SkuDetail\",\n  data() {\n    return {\n      // 查询结果\n      sku_data: {},\n\n      // sku_id\n      sku_id: 1\n    };\n  },\n  mounted() {\n      this.get_sku_detail()\n  },\n  methods: {\n    // 获取sku详情数据\n    get_sku_detail() {\n      let that = this;\n      that.$axios\n        .get(\"sku_detail/\" + that.sku_id)\n        .then(res => {\n          that.sku_data = res.data;\n        })\n        .catch(err => {\n          this.$notify.error({\n            title: \"获取sku详情失败\",\n            message: err.msg\n          });\n        });\n    }\n  },\n  components: {\n    Container,\n    Category\n  }\n};\n